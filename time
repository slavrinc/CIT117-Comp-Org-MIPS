# 04-01-2020
# CIT-117


.data
elapse: .asciiz "Time elapsed: "
ms: .asciiz " ms"

.text
li $v0, 4	# print label
la $a0, elapse
syscall

li $v0,30       # start timestamp
syscall 	

la $t0, ($a0)	# saving timestamp start data to $t0

li $v0,32  	#syscall function to sleep in milliseconds
li $a0,3000 	#3000 milliseconds
syscall

li $v0,30	# end timestamp
syscall

la $t1, ($a0)	# saving timestamp end data to $t1
sub $t2, $t1, $t0 # end timestamp - start timestamp = time elapsed
add $a0, $t2, $zero #loading time elapsed into $a0 to print

li $v0, 1 	#print $a0 = time elapsed
syscall

li $v0, 4	# print units
la $a0, ms
syscall

end: 
li $v0, 10 # exit syscall
syscall


